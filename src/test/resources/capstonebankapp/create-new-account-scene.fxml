<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CreateNewAccountController">
   <children>
      <Label fx:id="accountTypeLabel" alignment="CENTER_RIGHT" layoutX="137.0" layoutY="161.0" prefHeight="28.0" prefWidth="94.0" text="Account Type:" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <ChoiceBox fx:id="accountTypeChoiceBox" layoutX="250.0" layoutY="162.0" prefWidth="150.0" />
      <Label fx:id="titleLabel" alignment="CENTER" layoutX="143.0" layoutY="83.0" prefHeight="36.0" prefWidth="314.0" text="New Account Details" textFill="WHITE">
         <font>
            <Font name="System Bold" size="24.0" />
         </font></Label>
      <Label fx:id="signedInAsLabel" alignment="CENTER_RIGHT" layoutX="601.0" layoutY="1.0" prefHeight="26.0" prefWidth="78.0" text="Signed in as:" textFill="WHITE" />
      <Label fx:id="accountNameLabel" alignment="CENTER_RIGHT" layoutX="527.0" layoutY="20.0" prefHeight="26.0" prefWidth="152.0" textFill="#2e7ea6" />
      <Button fx:id="createNewAccountButton" layoutX="143.0" layoutY="260.0" mnemonicParsing="false" onAction="#handleCreateAccountButton" prefHeight="36.0" prefWidth="108.0" text="Create Account" />
      <Button fx:id="backButton" layoutX="310.0" layoutY="260.0" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="36.0" prefWidth="108.0" text="Back" />
      <Label fx:id="emptyErrorMessageLabel" layoutX="143.0" layoutY="208.0" prefHeight="26.0" prefWidth="283.0" textFill="#e40e0e" />
      <Label fx:id="alreadyExistingTypeErrorLabel" layoutX="95.0" layoutY="327.0" prefHeight="28.0" prefWidth="381.0" textFill="#e40e0e" />
      <Button fx:id="logOutButton" layoutX="610.0" layoutY="47.0" mnemonicParsing="false" onAction="#handleLogOutButton" prefHeight="26.0" prefWidth="67.0" text="Log out" />
   </children>
</AnchorPane>
