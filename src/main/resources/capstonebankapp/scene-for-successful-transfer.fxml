<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SuccessfulTransferController">
   <children>
      <Label fx:id="successfulTransferLabel" alignment="CENTER" layoutX="51.0" layoutY="141.0" prefHeight="58.0" prefWidth="497.0" text="Great news! Your transfer was successful!">
         <font>
            <Font name="System Bold" size="24.0" />
         </font></Label>
      <Button fx:id="backToDashboardButton" layoutX="221.0" layoutY="212.0" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="36.0" prefWidth="159.0" text="Back to Dashboard" />
      <Label fx:id="signedInAsLabel" alignment="CENTER_RIGHT" layoutX="498.0" prefHeight="28.0" prefWidth="101.0" text="Signed in as:" textFill="WHITE" />
      <Label fx:id="userNameLabel" layoutX="473.0" layoutY="22.0" prefHeight="28.0" prefWidth="126.0" textFill="#0451c4" />
      <Button fx:id="logOutButton" layoutX="519.0" layoutY="50.0" mnemonicParsing="false" onAction="#handleLogOutButton" prefHeight="28.0" prefWidth="80.0" text="Log out" />
   </children>
</AnchorPane>
