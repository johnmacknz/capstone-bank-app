<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <Label fx:id="usenameLabel" alignment="CENTER" layoutX="144.0" layoutY="135.0" prefHeight="30.0" prefWidth="85.0" style="-fx-background-color: #ADD8E6; -fx-text-fill: #00008B;" text="Username">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="passwordLabel" alignment="CENTER" layoutX="143.0" layoutY="191.0" prefHeight="30.0" prefWidth="85.0" style="-fx-background-color: #ADD8E6; -fx-text-fill: #00008B;" text="Password">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="userNameTextField" layoutX="247.0" layoutY="137.0" />
      <Button fx:id="loginButton" layoutX="159.0" layoutY="251.0" mnemonicParsing="false" onAction="#handleLoginButton" prefHeight="47.0" prefWidth="85.0" style="-fx-background-color: #ADD8E6; -fx-text-fill: #00008B;" text="Login">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="backButton" layoutX="284.0" layoutY="251.0" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="47.0" prefWidth="85.0" style="-fx-background-color: #ADD8E6; -fx-text-fill: #00008B;" text="Back">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>

      <PasswordField fx:id="passwordFieldLogin" layoutX="247.0" layoutY="193.0" />
      <Text fx:id="accountCreation" layoutX="253.0" layoutY="73.0" strokeType="OUTSIDE" strokeWidth="0.0" />
      <Label fx:id="titleLabel" alignment="CENTER" layoutX="136.0" layoutY="55.0" prefHeight="41.0" prefWidth="296.0" style="-fx-text-fill: #6082B6;" text="Welcome">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>

   </children>
</AnchorPane>
